{
    "builds": [
        {
            "src": "main.py", 
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.10"  // La versión de Python está fijada
            }
        }
    ], 
    "routes": [
        {
            "src": "/(.*)", 
            "dest": "main.py"
        }
    ]
}
